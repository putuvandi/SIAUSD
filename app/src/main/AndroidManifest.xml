<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.siausd">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/logo"
        android:label="@string/app_name"
        android:roundIcon="@drawable/logo"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true">
        <activity android:name=".Splash">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".SelamatDatangDosen" />
        <activity android:name=".SelamatDatangMhs" />
        <activity android:name=".BiodataDosen" />
        <activity android:name=".UbahPasswordDosen" />
        <activity android:name=".HomeDosen" />
        <activity android:name=".BiodataMahasiswa" />
        <activity android:name=".UbahPasswordMahasiswa" />
        <activity android:name=".Login"></activity>
        <activity android:name=".HomeMahasiswa"></activity>
    </application>

</manifest>